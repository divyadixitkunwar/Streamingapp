{
    "version": 2,
    "builds": [
      { "src": "src/index.ts", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "src/index.ts" }
    ],
    "functions": { "src": "src" },
    "hooks": {
      "build": {
        "command": "npm run build && npx prisma generate"
      }
    }
  }
  